class Solution {
    public List<List<Integer>> levelOrder(TreeNode root) {
    List<List<Integer>> res = new ArrayList<>();
        Queue<TreeNode> list = new LinkedList<>();
        if(root==null)
            return res;
        list.offer(root);
        while(!list.isEmpty()){
            int size = list.size();
            List<Integer> temp = new ArrayList<>();
            int i=0;
            while(i<size){
                TreeNode node = list.poll();
                temp.add(node.val);
                if(node.left!=null)
                    list.add(node.left);
                if(node.right!=null)
                    list.add(node.right);
                i++;
            }
            res.add(temp);
        }
        return res;
    }
}
